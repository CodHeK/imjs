{
  "name": "imjs",
  "version": "1.1.0",
  "description": "Client library for communication with InterMine web-services",
  "main": "js/main",
  "keywords": [
    "javascript",
    "webservice",
    "InterMine",
    "Bio-Informatics"
  ],
  "author": {
    "name": "Alex Kalderimis",
    "email": "alex@intermine.org",
    "twitter": "alexkalderimis"
  },
  "license": "LGPL",
  "repository": {
    "type": "git",
    "url": "git://github.com/alexkalderimis/imjs.git"
  },
  "dependencies": {
    "underscore": "stable",
    "underscore.deferred": "0.2.0",
    "clone": "stable",
    "buffered-response": "latest"
  },
  "engines": {
    "node": ">= 0.4.0"
  },
  "directories": {
    "tests": "test"
  },
  "scripts": {
    "pretest": "coffee -c -o lib src/*.coffee",
    "prepublish": "coffee -c -o lib src/*.coffee",
    "build-for-browser": "coffee -c -o build src/*.coffee && cat $(cat BUILD_ORDER) > js/im.js",
    "test": "C=0; T=0; for t in test/node/*.coffee; do T=$(($T + 1)); echo running $t; expresso $t && C=$(($C + 1)) && echo ok; done; test $C = $T && echo aok || echo tests failed"
  },
  "devDependencies": {
    "mocha": "latest",
    "grunt-simple-mocha": "latest",
    "should": "latest",
    "coffee-script": "~1.3.3",
    "codo": "~0.3.0",
    "grunt": ">= 0.3.17",
    "coffeelint": ">= 0.5.4",
    "grunt-coffeelint": ">= 0.0.3",
    "grunt-clean": ">= 0.3.0"
  }
}
